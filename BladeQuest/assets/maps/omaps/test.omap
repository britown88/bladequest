#!OBJ 2 3
\name$2x3$ <- newObject "name" 2 3

state1$2x3$ 0
name$2x3$ > createState >
setCollisionData true true true true >
addAction (startBattle "2rats")



\runstate1$2x3$ <- state1$2x3$ 0
